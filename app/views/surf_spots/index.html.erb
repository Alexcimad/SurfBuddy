<%# <header class="header">
  <%= render 'shared/search_bar' %>
<%# </header> %>
<section class="container-without-navbar">
    <div class="search row">

      <div class="map-right col-12 col-sm-6 col-md-8">
          <div id="map"
            data-markers="<%= @markers.to_json %>"
            data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
          </div>
      </div>

    </div>
    <div class="container">
      <div id="owl-carousel" class="owl-carousel owl-theme">
        <% @surf_spots.each do |spot| %>
        <div class="item">
-          <%= render 'shared/card_spot', surf_spot: spot, user: current_user %>
        </div>
        <% end %>
      </div>
    </div>
</section>
